// DO NOT EDIT
// This custom.scss is generated when <%= request.url %> is requested

<% for font in custom_css.fonts %>
<%= font %>
<% end %>

<% for keyframe in custom_css.keyframes %>
<%= keyframe %>
<% end %>

#<%= issue.handle %>-issue {
<%= custom_css.issue_scss.gsub(/^/, '  ') %>

  <% for style_path in custom_css.style_paths %>
  @import "<%= style_path %>";
  <% end %>
}
